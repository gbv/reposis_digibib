<ContentStoreSelectionRules default="IFS2">

  <store ID="SAM-FS">
    <type>jpeg</type>
    <type>png</type>
    <type>tiff</type>
    <type>gif</type>
    <type>bmp</type>
  </store>

  <store ID="STREAM">
    <type>mpeg4</type>
    <type>m4v</type>
    <type>m4a</type>
    <type>m4b</type>
    <type>avi</type>
    <type>wmv</type>
    <type>wma</type>
    <type>asf</type>
  </store>

</ContentStoreSelectionRules>

